<launch>
  <arg name="allow_trajectory_execution" value="true"/>
  <arg name="fake_execution" default="false"/>
  <arg name="config" default="true"/>
  <arg name="info" value="true"/>

  <arg name="use_gui" default="true" />

  <!-- Launch move_group -->
  <include file="$(find gemini_arm_moveit_config)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="$(arg allow_trajectory_execution)"/>
    <arg name="fake_execution" value="$(arg fake_execution)"/>
    <arg name="info" value="$(arg info)"/>
  </include>

  <!-- For real execution -->
  <include file="$(find gemini_arm_moveit_config)/launch/trajectory_execution.launch.xml"/>

  <!-- Launch moveit rviz -->
  <include file="$(find gemini_arm_moveit_config)/launch/moveit_rviz.launch">
    <arg name="config" value="$(arg config)"/>
  </include>
</launch>

